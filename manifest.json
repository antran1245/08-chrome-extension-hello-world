{
  "name": "Hello Extension",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "storage",
    "windows"
  ],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "images/hello_extension.png"
  },
  "host_permissions": [
    "http://127.0.0.1:5000/"
  ]
}